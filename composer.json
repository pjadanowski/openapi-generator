{
  "name": "pjadanowski/openapi-generator",
  "description": "Generate OpenAPI 3.0+ documentation from Laravel routes, controllers, and Spatie Data DTOs",
  "type": "library",
  "keywords": [
    "laravel",
    "spatie",
    "data",
    "openapi",
    "swagger",
    "documentation",
    "routes",
    "controllers",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pawel Jadanowski",
      "email": "pjadanowski@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "spatie/laravel-data": "^3.0|^4.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "cebe/php-openapi": "^1.7",
    "symfony/finder": "^6.0|^7.0",
    "spatie/php-structure-discoverer": "^2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "orchestra/testbench": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pjadanowski\\OpenApiGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pjadanowski\\OpenApiGenerator\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pjadanowski\\OpenApiGenerator\\OpenApiGeneratorServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
